{
  "typescript.tsdk": "node_modules/typescript/lib",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "explicit"
  },
  "eslint.options": {
    "extensions": [".js", ".ts", ".json"]
  },
  "eslint.validate": [
    "javascript",
    "typescript",
    "json",
    "jsonc"
  ]
}